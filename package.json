    "babel-loader": "^9.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "fast-xml-parser": "^5.2.3",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jscodeshift": "^0.15.2",
    "minimatch": "^9.0.5",
    "prettier": "^3.5.3",
    "sharp": "^0.34.1",
    "simple-git-hooks": "^2.9.0",
    "storybook": "^8.6.14",
