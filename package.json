    "cookie": "^1.0.2",
    "framer-motion": "^12.7.4",
    "gray-matter": "^4.0.3",
    "lru-cache": "^11.2.2",
    "lucide-react": "^0.501.0",
    "next": "^15.4.6",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.239.0",
    "posthog-node": "^5.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.56.0",
    "react-intersection-observer": "^9.16.0",
