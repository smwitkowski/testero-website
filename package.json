{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate:sitemap": "ts-node scripts/generate-sitemap.js",
    "generate:social-images": "node scripts/generate-social-images.js || echo 'Warning: Social image generation failed, continuing build'",
