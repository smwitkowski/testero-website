{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_NO_WARNINGS=1 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test:eslint-rules": "jest -c eslint/jest.config.ts",
    "generate:sitemap": "NODE_NO_WARNINGS=1 ts-node scripts/generate-sitemap.js",
    "generate:social-images": "NODE_NO_WARNINGS=1 node scripts/generate-social-images.js || echo 'Warning: Social image generation failed, continuing build'",
    "generate:favicons": "NODE_NO_WARNINGS=1 tsx scripts/generate-favicons.ts",
    "generate:seo": "npm run generate:sitemap && npm run generate:social-images",
    "prebuild": "npm run generate:favicons && npm run generate:seo",
    "setup:gcp-storage": "NODE_NO_WARNINGS=1 node scripts/setup-gcp-storage.js",
    "gcp:enable": "cross-env USE_GCP_STORAGE=true npm run generate:social-images",
    "indexnow:submit": "NODE_NO_WARNINGS=1 node scripts/submit-to-indexnow.js",
    "validate:content": "NODE_NO_WARNINGS=1 node scripts/validate-content.js",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:verify": "node -e \"const fs=require('fs'); if(!fs.existsSync('.storybook')){console.error('Storybook not found: bootstrap it before CI (Task 5).'); process.exit(2)} else {console.log('Storybook config found.')}\"",
    "storybook:preview": "http-server storybook-static -p 6007 -c-1",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --runInBand --ci",
    "e2e": "playwright test",
    "e2e:ci": "playwright test --reporter=list,html",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "codemod:ds:run": "tsx scripts/codemods/ds/run-all.ts",
    "codemod:ds:dry": "tsx scripts/codemods/ds/run-all.ts --dry",
    "lint:design": "node scripts/guards/design-lints.js",
    "prepare": "node scripts/setup-tailwindcss-eslint-shim.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google-cloud/storage": "^7.16.0",
    "@hookform/resolvers": "^5.0.1",
    "@mui/material": "^7.0.2",
    "@openrouter/ai-sdk-provider": "^0.7.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
