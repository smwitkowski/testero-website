    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.0",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@upstash/redis": "^1.35.3",
